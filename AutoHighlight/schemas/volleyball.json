{
  "description": "Volleyball highlights analyzer \u2013 segments video at key narrative moments and extracts flat metadata for each clip.",
  "baseAnalyzerId": "prebuilt-videoAnalyzer",
  "config": {
    "returnDetails": true,
    "segmentationMode": "custom",
    "segmentationDefinition": "Segment the video at each significant volleyball moment that contributes to a compelling highlight reel: Aces, Blocks, Attack Kills, Momentum Shifts, Crowd Cheers, Substitutions, Timeouts, Coach Reactions, Challenges, Violations, or other narrative-driven events. Each segment should capture exactly one distinct moment. Aim for a target clip density of 4.0 clips per minute and a total highlights duration of about 100 seconds. No personalization is required."
  },
  "fieldSchema": {
    "fields": {
      "Segments": {
        "type": "array",
        "method": "generate",
        "items": {
          "type": "object",
          "properties": {
            "SegmentId": {
              "type": "string",
              "method": "generate",
              "description": "Unique identifier for this segment."
            },
            "StartTimeMs": {
              "type": "integer",
              "method": "generate",
              "description": "Start time of this segment in milliseconds."
            },
            "EndTimeMs": {
              "type": "integer",
              "method": "generate",
              "description": "End time of this segment in milliseconds."
            },
            "Description": {
              "type": "string",
              "method": "generate",
              "description": "Natural language description of what happens in this segment."
            },
            "SegmentType": {
              "type": "string",
              "method": "classify",
              "description": "Overall context of this segment.",
              "enum": [
                "LiveRally",
                "Replay",
                "Stoppage",
                "Timeout",
                "CoachHuddle",
                "Challenge",
                "Advertisement",
                "Studio",
                "Other"
              ]
            },
            "PlayEvent": {
              "type": "string",
              "method": "classify",
              "description": "Primary moment in this segment.",
              "enum": [
                "Serve",
                "Ace",
                "Kill",
                "Block",
                "Dig",
                "Set",
                "Substitution",
                "Challenge",
                "Violation",
                "MomentumShift",
                "Celebration",
                "None",
                "Other"
              ]
            },
            "HomeTeamName": {
              "type": "string",
              "method": "generate",
              "description": "Home team name from the on-screen scoreboard."
            },
            "AwayTeamName": {
              "type": "string",
              "method": "generate",
              "description": "Away team name from the on-screen scoreboard."
            },
            "HomeTeamScore": {
              "type": "integer",
              "method": "generate",
              "description": "Home team\u2019s score at the start of this segment."
            },
            "AwayTeamScore": {
              "type": "integer",
              "method": "generate",
              "description": "Away team\u2019s score at the start of this segment."
            },
            "CurrentSet": {
              "type": "string",
              "method": "classify",
              "description": "Current set number being played.",
              "enum": [
                "Set1",
                "Set2",
                "Set3",
                "Set4",
                "Set5",
                "Unknown"
              ]
            },
            "MatchTime": {
              "type": "string",
              "method": "generate",
              "description": "Approximate elapsed match time at start of this segment (MM:SS)."
            },
            "ScoringPlayer": {
              "type": "string",
              "method": "generate",
              "description": "Name of the player who scored the point (if applicable)."
            },
            "AssistingPlayer": {
              "type": "string",
              "method": "generate",
              "description": "Name of the player who set up the scoring play (if applicable)."
            },
            "CardPlayer": {
              "type": "string",
              "method": "generate",
              "description": "Name of the player who received a card for misconduct (if applicable)."
            },
            "CardColor": {
              "type": "string",
              "method": "classify",
              "description": "Color of the card shown.",
              "enum": [
                "Yellow",
                "Red"
              ]
            },
            "CommentaryExcitement": {
              "type": "string",
              "method": "classify",
              "description": "Estimated excitement level of the commentary.",
              "enum": [
                "Low",
                "Medium",
                "High",
                "Very High"
              ]
            },
            "CrowdNoiseLevel": {
              "type": "string",
              "method": "classify",
              "description": "Estimated crowd noise level.",
              "enum": [
                "Low",
                "Medium",
                "High"
              ]
            },
            "InvolvedPlayers": {
              "type": "string",
              "method": "generate",
              "description": "Comma-separated list of players involved in the rally."
            },
            "HighlightScore": {
              "type": "number",
              "method": "generate",
              "description": "AI-generated highlight-worthiness score between 0.0 and 1.0."
            }
          }
        }
      }
    }
  }
}